{
	"name": "rogue-lineage-tycoon",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},
			"Source": {
				"$className": "Folder",
				"Shared": {
					"$path": "out/shared"
				},
				"Client": {
					"$path": "out/client"
				}
			}
		},
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"Source": {
				"$path": "out/scripts/server"
			}
		},
		"ServerStorage": {
			"$className": "ServerStorage",
			"Source": {
				"$path": "out/server"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterCharacterScripts": {
				"$className": "StarterCharacterScripts",
				"Source": {
					"$path": "out/scripts/character"
				}
			},
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"Source": {
					"$path": "out/scripts/player"
				}
			}
		}
	}
}
